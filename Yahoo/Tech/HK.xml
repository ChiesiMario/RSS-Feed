            <guid isPermaLink="false">https://tw.news.yahoo.com/%E5%BE%AE%E8%BB%9F%E4%BC%BC%E4%B9%8E%E6%AD%A3%E5%9C%A8%E6%B8%AC%E8%A9%A6%E5%90%AB%E5%BB%A3%E5%91%8A%E7%9A%84%E5%85%8D%E8%B2%BB-office%EF%BC%8C%E4%BD%86%E5%83%85%E9%99%90-windows-%E7%89%88%E6%9C%AC-033724718.html</guid>
