            <link>https://tw.news.yahoo.com/%E5%8F%B0%E6%BE%8E%E4%B8%89%E8%99%9F-%E6%B5%B7%E7%BA%9C%E5%85%A8%E6%96%B7-%E6%95%B8%E7%99%BC%E9%83%A8-%E7%B7%8A%E6%80%A5%E8%AA%BF%E5%BA%A6%E5%82%99%E6%8F%B4-%E4%BF%83%E5%8A%A0%E9%80%9F%E4%BF%AE%E5%BE%A9%E4%BD%9C%E6%A5%AD-044511939.html</link>
            <guid isPermaLink="false">https://tw.news.yahoo.com/%E5%8F%B0%E6%BE%8E%E4%B8%89%E8%99%9F-%E6%B5%B7%E7%BA%9C%E5%85%A8%E6%96%B7-%E6%95%B8%E7%99%BC%E9%83%A8-%E7%B7%8A%E6%80%A5%E8%AA%BF%E5%BA%A6%E5%82%99%E6%8F%B4-%E4%BF%83%E5%8A%A0%E9%80%9F%E4%BF%AE%E5%BE%A9%E4%BD%9C%E6%A5%AD-044511939.html</guid>
            <pubDate>Tue, 25 Feb 2025 04:45:11 GMT</pubDate>
            <author>民视新闻网 @民视</author>
            <category>中华电信</category>
